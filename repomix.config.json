{
  "$schema": "https://repomix.com/schemas/latest/schema.json",
  "input": {
    "maxFileSize": 52428800
  },
  "output": {
    "filePath": "docs/repomix.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": false,
    "directoryStructure": true,
    "files": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "compress": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": true,
    "git": {
      "sortByChanges": false,
      "sortByChangesMaxCommits": 100,
      "includeDiffs": false
    }
  },
  "include": [
    // "package.json",
    // "tsconfig.json",
    // "src",

    // "repograph/package.json",
    // "repograph/tsconfig.json",
    // "repograph/tsconfig.build.json",
    // "repograph/src"
    "repograph/test/e2e/cli.test.ts",
    "repograph/test/unit/high-level.test.ts"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [

    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
