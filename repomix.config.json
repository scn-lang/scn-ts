{
  "$schema": "https://repomix.com/schemas/latest/schema.json",
  "input": {
    "maxFileSize": 52428800
  },
  "output": {
    "filePath": "docs/repomix.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": false,
    "directoryStructure": true,
    "files": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "compress": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": true,
    "git": {
      "sortByChanges": false,
      "sortByChangesMaxCommits": 100,
      "includeDiffs": false
    }
  },
  "include": [
    "package.json",
    "tsconfig.json",
    // "src",
    // "docs/test.plan.md",
    "docs/scn.readme.md",
    "test",

    "repograph/package.json",
    "repograph/tsconfig.json",
    "repograph/tsconfig.build.json",
    // "repograph/src",

    "repograph/test/e2e/cli.test.ts",
    "repograph/test/integration/multi-language.test.ts",
    "repograph/test/unit/codenode-qualifiers.test.ts",
    "repograph/test/unit/analyze.test.ts",

    "repograph/src/pipeline/analyze.ts",
    "repograph/src/types.ts",
    "repograph/src/tree-sitter/queries.ts",
    "repograph/src/index.ts",
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [

    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
